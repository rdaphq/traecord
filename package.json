{
  "main": "./extension.js",
  "name": "rdaphq-traecord",
  "displayName": "Traecord: Discord Presence for Trae",
  "publisher": "rdaphq",
  "description": "Show what you're coding in Trae IDE on your Discord activity.",
  "version": "1.0.7",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "author": {
    "name": "rdaphq",
    "email": "rdaphq@gmail.com",
    "url": "https://github.com/rdaphq"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdaphq/traecord"
  },
  "bugs": {
    "url": "https://github.com/rdaphq/traecord/issues"
  },
  "homepage": "https://github.com/rdaphq/traecord#readme",
  "license": "MIT",
  "icon": "icon.png",
  "keywords": [
    "rpc",
    "vscode",
    "traeide",
    "discord",
    "presence",
    "activity",
    "discord rpc",
    "discord presence",
    "traecord",
    "trae"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "traecord.toggle",
        "title": "Toggle Traecord Connection"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Traecord Settings",
      "properties": {
        "traecord.showFileName": {
          "type": "boolean",
          "default": true,
          "description": "Show file name in Discord status"
        },
        "traecord.showWorkspace": {
          "type": "boolean",
          "default": true,
          "description": "Show workspace name in Discord status"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.60.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "files": [
    "extension.js",
    "rpc-client.js"
  ],
  "dependencies": {
    "discord-rpc": "^4.0.1"
  }
}
